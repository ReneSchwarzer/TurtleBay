<?xml version="1.0" encoding="utf-8" ?>
<config version = "1">

	<!-- Loggingeinstellungen Off, Override, Append-->
	<log modus="Off" path="/var/log/" encoding="iso-8859-15" filename="turtlebay.log" timepattern="dd.MM.yyyy HH:mm:ss" />

	<endpoint uri="http://localhost/"/>

	<limit>
		<connectionlimit>300</connectionlimit>
		<uploadlimit>3000000000</uploadlimit>
	</limit>

	<assets>./assets</assets>
	<data>./data</data>
	<contextpath></contextpath>
	<stagedirectory>./</stagedirectory>

</config>